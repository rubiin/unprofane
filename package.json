{
  "name": "unprofane",
  "version": "0.1.2",
  "description": "Library to detect and redact profane words",
  "type": "module",
  "packageManager": "pnpm@7.2.1",
  "author": "Rubin Bhandari <roobin.bhandari@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/rubiin/unprofane#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubiin/unprofane.git"
  },
  "bugs": "https://github.com/rubiin/unprofane/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup && copyfiles -f ./src/static/**/*.json ./dist/static/languages",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && clean-publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.33.1",
    "@antfu/ni": "^0.18.8",
    "@types/node": "^18.11.13",
    "bumpp": "^8.2.1",
    "clean-publish": "^4.0.2",
    "eslint": "^8.29.0",
    "esno": "^0.16.3",
    "rimraf": "^3.0.2",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "unbuild": "^1.0.2",
    "vite": "^4.0.0",
    "vitest": "^0.25.7",
    "copyfiles": "^2.1.1"
  },
  "clean-publish": {
    "cleanDocs": true,
    "cleanComments": true
  }
}
